{
  "name": "@viralganatra/octopus-tariff",
  "author": "Viral Ganatra <viral@viralganatra.com>",
  "license": "MIT",
  "version": "1.0.0",
  "description": "Smart Tariff Calculator",
  "scripts": {
    "deploy:dev": "sst deploy --stage development",
    "deploy:prod": "sst deploy --stage production",
    "dev": "sst dev",
    "format": "biome format --write",
    "lint": "biome lint",
    "prepare": "husky",
    "sso": "aws sso login --sso-session=octopus",
    "test": "TZ=UTC vitest",
    "typecheck": "tsc -b",
    "generate:creds": "sh ./scripts/export-sso-creds.sh"
  },
  "type": "module",
  "dependencies": {
    "@aws-lambda-powertools/logger": "^2.19.1",
    "@aws-sdk/client-dynamodb": "^3.804.0",
    "@aws-sdk/client-sqs": "^3.804.0",
    "@aws-sdk/util-dynamodb": "^3.804.0",
    "@date-fns/tz": "^1.2.0",
    "@pulumi/aws": "^6.80.0",
    "camelcase-keys": "^9.1.3",
    "date-fns": "^4.1.0",
    "graffle": "8.0.0-next.157",
    "graphql": "^16.11.0",
    "sst": "3.9.27",
    "type-fest": "^4.41.0",
    "zod": "^3.24.4",
    "zod-validation-error": "^3.4.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/aws-lambda": "8.10.147",
    "@types/node": "^22.15.14",
    "aws-sdk-client-mock": "^4.1.0",
    "aws-sdk-client-mock-jest": "^4.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "msw": "^2.7.6",
    "sst": "^3.14.11",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3"
  },
  "lint-staged": {
    "*": "pnpm format"
  }
}
