exports[`API Data > should fetch todays consumption 2`] = `
{
  "query": "
    query smartMeterTelemetry(
      $deviceId: String!,
      $start: DateTime,
      $end: DateTime,
      $grouping: TelemetryGrouping
    ) {
      smartMeterTelemetry(
        deviceId: $deviceId,
        start: $start,
        end: $end,
        grouping: $grouping
      ) {
        readAt
        consumptionDelta
        costDeltaWithTax
      }
    }
  ",
  "variables": {
    "deviceId": "deviceId",
    "end": "2025-03-03T23:59:59Z",
    "grouping": "HALF_HOURLY",
    "start": "2025-03-03T00:30:00Z",
  },
}
`;
